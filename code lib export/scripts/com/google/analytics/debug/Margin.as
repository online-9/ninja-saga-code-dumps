package com.google.analytics.debug
{
   public class Margin
   {
       
      
      public var top:int;
      
      public var left:int;
      
      public var bottom:int;
      
      public var right:int;
      
      public function Margin(top:int = 0, bottom:int = 0, left:int = 0, right:int = 0)
      {
         super();
         this.top = top;
         this.bottom = bottom;
         this.left = left;
         this.right = right;
      }
   }
}
